            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=leetcode-cli"
                ],
                "filter": {
                    "name": "leetcode-cli",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'leetcode'",
            "env": {
                "RUST_BACKTRACE": "1"},
            "cargo": {
                "args": [
                    "build",
                    "--bin=leetcode",
                    "--package=leetcode-cli"
                ],
                "filter": {
                    "name": "leetcode",
                    "kind": "bin"
                }
            },
            "args": ["p","159"],
